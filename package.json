  "version": "2.0.0",
  "author": "Genuifx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Genuifx/postcss-pxtorpx-pro.git"
  },
  "bugs": "https://github.com/Genuifx/postcss-pxtorpx-pro/issues",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jasmine-node spec"
  },
  "husky": {
